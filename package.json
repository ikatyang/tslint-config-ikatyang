{
  "name": "tslint-config-ikatyang",
  "version": "2.5.1",
  "description": "tslint config for ikatyang",
  "main": "lib/tslint.json",
  "repository": "https://github.com/ikatyang/tslint-config-ikatyang",
  "author": "ikatyang",
  "license": "MIT",
  "scripts": {
    "prepublish": "yarn run build",
    "prebuild": "rm -rf ./lib && mkdir ./lib",
    "build": "node ./tools/generate.js > ./lib/tslint.json",
    "lint": "tslint -p .",
    "test": "jest",
    "release": "standard-version"
  },
  "dependencies": {
    "tslint-consistent-codestyle": "~1.8.0",
    "tslint-eslint-rules": "~4.1.0",
    "tslint-plugin-ikatyang": "~1.1.1"
  },
  "devDependencies": {
    "@types/jest": "23.1.1",
    "@types/node": "10.3.4",
    "@types/prettier": "1.13.1",
    "jest": "23.1.0",
    "jest-snapshot-serializer-raw": "1.0.0",
    "prettier": "1.13.5",
    "prettier-config-ikatyang": "1.1.1",
    "standard-version": "4.4.0",
    "tslint": "5.8.0",
    "tslint-plugin-prettier": "1.3.0",
    "typescript": "2.9.2"
  },
  "peerDependencies": {
    "tslint": "^5.8.0"
  },
  "files": [
    "/lib/**/*"
  ]
}
