  "devDependencies": {
    "standard-version": "4.2.0",
    "tslint": "5.5.0",
    "typescript": "2.4.2"
  },
  "peerDependencies": {
    "tslint": "^5.5.0"
  },
  "files": [
    "/lib/**/*"
