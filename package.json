{
  "name": "tslint-config-ikatyang",
  "version": "1.0.1",
  "description": "tslint config for ikatyang",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/ikatyang/tslint-config-ikatyang",
  "author": "ikatyang",
  "license": "MIT",
  "scripts": {
    "prepublish": "npm run build && npm run build-prettier",
    "lint": "tslint -p ./tsconfig.json --type-check",
    "test": "npm run lint",
    "prebuild": "rm -rf ./lib",
    "build": "tsc -p ./tsconfig.build.json",
    "build-prettier": "tsc -p ./tsconfig.prettier.json",
    "format": "gulp format",
    "format-check": "gulp format-check"
  },
  "dependencies": {
    "tslint-consistent-codestyle": "^1.5.1",
    "tslint-eslint-rules": "^4.1.0"
  },
  "devDependencies": {
    "@types/gulp": "3.8.33",
    "@types/prettier": "1.5.0",
    "gulp": "3.9.1",
    "gulp-plugin-prettier": "1.0.0",
    "prettier": "1.5.3",
    "ts-node": "3.2.1",
    "tslint": "5.5.0",
    "tslint-config-prettier": "1.1.0",
    "typescript": "2.4.2"
  },
  "peerDependencies": {
    "tslint": "^5.5.0"
  },
  "files": [
    "/lib/**/*",
    "/prettier.js",
    "/prettier.d.ts"
  ]
}
