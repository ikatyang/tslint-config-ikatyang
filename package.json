{
  "name": "tslint-config-ikatyang",
  "version": "2.3.0",
  "description": "tslint config for ikatyang",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/ikatyang/tslint-config-ikatyang",
  "author": "ikatyang",
  "license": "MIT",
  "scripts": {
    "prepublish": "yarn run build",
    "lint": "tslint -p ./tsconfig.json --type-check",
    "test": "tslint -p ./tsconfig.test.json --type-check",
    "build": "rm -rf ./lib && tsc -p ./tsconfig.build.json",
    "release": "standard-version"
  },
  "dependencies": {
    "tslint-consistent-codestyle": "~1.6.0",
    "tslint-eslint-rules": "~4.1.0",
    "tslint-plugin-ikatyang": "~1.0.0"
  },
  "devDependencies": {
    "@types/node": "8.0.25",
    "prettier": "1.5.3",
    "prettier-config-ikatyang": "1.1.1",
    "standard-version": "4.2.0",
    "tslint": "5.7.0",
    "tslint-config-prettier-ext": "1.5.0",
    "tslint-plugin-prettier": "1.1.0",
    "typescript": "2.4.2"
  },
  "peerDependencies": {
    "tslint": "^5.7.0"
  },
  "files": [
    "/lib/**/*"
  ]
}
